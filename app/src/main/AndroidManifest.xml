        android:supportsRtl="true"
        android:theme="@style/Theme.ImplementAllAndroid">
        <activity android:name=".activity.StopWatchServiceActivity"></activity>
        <activity android:name=".activity.DaggerWithDiActivity">
            <intent-filter>
